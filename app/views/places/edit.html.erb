<%#= render "form" %>
      <%= simple_form_for @place do |f| %>
        <%= f.input :title %>
        <%= f.input :category %>
        <%= f.input :address %>
        <%= f.input :min_age %>
        <%= f.input :max_age %>
        <%= f.input :indoor, as: :select %>
        <%= f.input :photo %>
        <%= f.button :submit, class: 'btn btn-primary' %>
      <% end %>



